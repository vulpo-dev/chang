{
  "db_name": "PostgreSQL",
  "query": "\r\nwith insert_history as (\r\n\tinsert into chang.task_history(task_id, from_state, to_state)\r\n\tselect $1 as task_id\r\n\t     , chang.tasks.state as from_state \r\n\t     , 'running' as to_state\r\n      from chang.tasks\r\n     where id = $1 \r\n       and state = 'scheduled' \r\n\treturning task_id as id\r\n)\r\nupdate chang.tasks\r\n   set state = 'running'\r\n     , attempted_at = now()\r\n     , attempt = attempt + 1\r\n where id in (select * from insert_history)\r\nreturning id\r\n        , state as \"state: TaskState\"\r\n        , attempt\r\n        , scheduled_at\r\n        , max_attempts\r\n        , attempted_by\r\n        , tags\r\n        , kind\r\n        , args\r\n        , priority\r\n        , queue\r\n        , depends_on\r\n        , dependend_id\r\n",
